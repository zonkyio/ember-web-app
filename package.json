{
  "name": "ember-web-app",
  "version": "4.0.0",
  "description": "This Ember addon helps you configure and manage the Web App Manifest to create a Progressive Web App",
  "keywords": [
    "PWA",
    "android",
    "ember-addon",
    "ios",
    "iphone",
    "manifest.json",
    "progressive web applications"
  ],
  "homepage": "https://zonkyio.github.io/ember-web-app",
  "repository": "https://github.com/zonkyio/ember-web-app.git",
  "license": "MIT",
  "author": "Santiago Ferreira",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "release": "release-it",
    "changelog": "lerna-changelog",
    "fast-test": "SKIP_ACCEPTANCE=true yarn node-test",
    "node-test": "nyc --reporter=lcov mocha --recursive node-tests",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint . --cache",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "test:one": "ember try:one",
    "deploy": "ember deploy production --activate --verbose"
  },
  "dependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "ember-cli-babel": "^7.22.1",
    "jsdom": "^16.4.0",
    "web-app-manifest-validator": "^1.0.0",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "ember-cli": "~3.21.1",
    "ember-cli-addon-docs": "^0.9.0",
    "ember-cli-addon-docs-esdoc": "^0.2.3",
    "ember-cli-addon-tests": "^0.11.1",
    "ember-cli-blueprint-test-helpers": "^0.19.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.11.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lerna-changelog": "^1.0.1",
    "lint-staged": "^10.3.0",
    "loader.js": "^4.7.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "qunit-dom": "^1.4.0",
    "release-it": "^14.0.2",
    "release-it-lerna-changelog": "^2.3.0"
  },
  "engines": {
    "node": "10.* || 12.* || >= 14.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "broccoli-asset-rev"
  },
  "nyc": {
    "all": true,
    "include": [
      "index.js",
      "blueprints/ember-web-app/index.js",
      "lib/*"
    ]
  },
  "volta": {
    "node": "10.22.0",
    "yarn": "1.22.4"
  }
}
